<?xml version="1.0" encoding="UTF-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee                http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

  <!-- Servlets -->
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>com.appspot.deedleit.server.api.Login</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/api/v2/login</url-pattern>
  </servlet-mapping>
  
  
  
  <servlet>
    <servlet-name>add</servlet-name>
    <servlet-class>com.appspot.deedleit.server.api.Add</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>add</servlet-name>
    <url-pattern>/api/v2/add</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
    <servlet-name>like</servlet-name>
    <servlet-class>com.appspot.deedleit.server.api.Like</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>like</servlet-name>
    <url-pattern>/api/v2/like</url-pattern>
  </servlet-mapping>
  
    <servlet>
    <servlet-name>follow</servlet-name>
    <servlet-class>com.appspot.deedleit.server.api.Follow</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>follow</servlet-name>
    <url-pattern>/api/v2/follow</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>greetServlet</servlet-name>
    <servlet-class>com.appspot.deedleit.server.GreetingServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>greetServlet</servlet-name>
    <url-pattern>/deedle/greet</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>Deedle.html</welcome-file>
  </welcome-file-list>

  <servlet>
    <servlet-name>SystemServiceServlet</servlet-name>
    <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
    <init-param>
      <param-name>services</param-name>
      <param-value/>
    </init-param>
  </servlet>

  <servlet-mapping>
    <servlet-name>SystemServiceServlet</servlet-name>
    <url-pattern>/_ah/spi/*</url-pattern>
  </servlet-mapping>

</web-app>